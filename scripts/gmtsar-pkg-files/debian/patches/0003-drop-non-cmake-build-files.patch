-AC_PROG_INSTALL
-AC_PROG_AWK
-AC_PROG_RANLIB
-dnl
-dnl ------------------------------------------------------------------
-dnl Get current OS and chip type and set LAPACK
-dnl ------------------------------------------------------------------
-dnl
-os=`uname -s`
-AC_MSG_CHECKING(for LAPACK library)
-if test "X$os" = "XDarwin" ; then
-	MT="-mt"
-	osr=`uname -r | cut -d. -f1`
