-	ORBITS_DIR=$rootdir
-	AC_MSG_RESULT([Not specified, default to $ORBITS_DIR])
-elif test "X$with_orbits_dir" = "Xno"; then
-	ORBITS_DIR=$rootdir
-	AC_MSG_RESULT([Not specified, default to $ORBITS_DIR])
-else
